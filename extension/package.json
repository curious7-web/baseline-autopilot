{
  "name": "baseline-autopilot-extension",
  "displayName": "Baseline Autopilot",
  "description": "Detects and provides fixes for non-baseline CSS and JavaScript features.",
  "version": "1.0.0",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Linters",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:css"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baselineAutopilot.autofix",
        "title": "Apply Baseline Autofix",
        "category": "Baseline Autopilot"
      }
    ]
  },
  "dependencies": {
    "acorn": "^8.15.0",
    "escodegen": "^2.1.0",
    "estraverse": "^5.3.0",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "web-features": "^3.0.0"
  }
}